<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/superusers"
        tools:context=".SuperUsersActivity">


    <Button
            android:text="Sterge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_delete1"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.211" app:layout_constraintVertical_bias="0.499"
            android:background="#FADBDB"/>
    <Button
            android:text="Sterge "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_delete2" android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="252dp"
            app:layout_constraintVertical_bias="0.505" android:background="#8894F7"/>
    <TextView
            android:text="Sterge din coada 1"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:id="@+id/textView2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="122dp" android:layout_marginBottom="26dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_delete1" app:layout_constraintEnd_toStartOf="@+id/textPers"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="1.0" android:textColor="#FAFAFA"/>
    <TextView
            android:text="Sterge din coada 2"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:id="@+id/textPers" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="122dp" android:layout_marginBottom="26dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_delete2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintVertical_bias="1.0" android:textColor="#000000"
            android:textColorHighlight="#000E0404"/>
</android.support.constraint.ConstraintLayout>